<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/bg_background"
    tools:context=".Ui.Patient_all.Setting_P.Help">

    <RelativeLayout
        android:id="@+id/help"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_70sdp"
        android:background="@drawable/bg_sing_up"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_marginLeft="@dimen/_90sdp"
            android:layout_marginTop="@dimen/_15sdp"
            android:fontFamily="@font/cairo_bold"
            android:text=" المساعدة "
            android:textColor="@color/white"
            android:textSize="@dimen/_20sdp" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/help_app"
        android:layout_width="@dimen/_280sdp"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/_10sdp"
        android:layout_marginTop="@dimen/_20sdp"
        android:layout_marginRight="@dimen/_10sdp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/help">

        <TextView
            android:id="@+id/help_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginRight="20dp"
            android:fontFamily="@font/cairo_bold"
            android:paddingTop="@dimen/_20sdp"
            android:paddingBottom="@dimen/_20sdp"
            android:text="عن التطبيق  "
            android:textAlignment="textStart"
            android:textSize="@dimen/_13sdp" />

        <ImageView
            android:id="@+id/help_btn_back"
            android:layout_width="@dimen/_35sdp"
            android:layout_height="@dimen/_35sdp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginTop="@dimen/_15sdp"
            android:background="@drawable/round_back_light"
            android:backgroundTint="?attr/bg_background"
            android:elevation="@dimen/_1sdp"
            android:paddingLeft="@dimen/_12sdp"
            android:paddingRight="@dimen/_5sdp"
            android:src="@drawable/ic_arrow_back_p" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/about_app"
        android:layout_width="@dimen/_280sdp"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/_10sdp"
        android:layout_marginRight="@dimen/_10sdp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/help_app">

        <View style="@style/Divider.Horizontal" />

        <TextView
            android:id="@+id/about_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginRight="20dp"
            android:fontFamily="@font/cairo_bold"
            android:paddingTop="@dimen/_20sdp"
            android:paddingBottom="@dimen/_20sdp"
            android:text="عن التطبيق  "
            android:textAlignment="textStart"
            android:textSize="@dimen/_13sdp" />

        <ImageView
            android:id="@+id/about_btn_back"
            android:layout_width="@dimen/_35sdp"
            android:layout_height="@dimen/_35sdp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginTop="@dimen/_15sdp"
            android:background="@drawable/round_back_light"
            android:backgroundTint="?attr/bg_background"

            android:elevation="@dimen/_1sdp"
            android:paddingLeft="@dimen/_12sdp"
            android:paddingRight="@dimen/_5sdp"
            android:src="@drawable/ic_arrow_back_p" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/agreement_app"
        android:layout_width="@dimen/_280sdp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_10sdp"
        android:layout_marginRight="@dimen/_10sdp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/about_app">

        <View style="@style/Divider.Horizontal" />

        <TextView
            android:id="@+id/contract_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:fontFamily="@font/cairo_bold"
            android:paddingTop="@dimen/_20sdp"
            android:paddingBottom="@dimen/_20sdp"
            android:text="اتفاقية الترخيص "
            android:textAlignment="textStart"
            android:textSize="@dimen/_13sdp" />

        <ImageView
            android:id="@+id/contract_btn_back"
            android:layout_width="@dimen/_35sdp"
            android:layout_height="@dimen/_35sdp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginTop="@dimen/_15sdp"
            android:background="@drawable/round_back_light"
            android:backgroundTint="?attr/bg_background"
            android:elevation="@dimen/_1sdp"
            android:paddingLeft="@dimen/_12sdp"
            android:paddingRight="@dimen/_5sdp"
            android:src="@drawable/ic_arrow_back_p" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/Privacy_app"
        android:layout_width="@dimen/_280sdp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_10sdp"
        android:layout_marginRight="@dimen/_10sdp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/agreement_app">

        <View style="@style/Divider.Horizontal" />

        <TextView
            android:id="@+id/notification_sound_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginRight="20dp"
            android:fontFamily="@font/cairo_bold"
            android:paddingTop="@dimen/_20sdp"
            android:paddingBottom="@dimen/_20sdp"
            android:text="شروط الخصوصية  "
            android:textAlignment="textStart"
            android:textSize="@dimen/_13sdp" />

        <ImageView
            android:id="@+id/notification_btn_back"
            android:layout_width="@dimen/_35sdp"
            android:layout_height="@dimen/_35sdp"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginTop="@dimen/_15sdp"
            android:background="@drawable/round_back_light"
            android:backgroundTint="?attr/bg_background"
            android:elevation="@dimen/_1sdp"
            android:paddingLeft="@dimen/_12sdp"
            android:paddingRight="@dimen/_5sdp"
            android:src="@drawable/ic_arrow_back_p" />
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>