<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Ui.Patient_all.Setting_P.Edit_Profile_Fragmane.Diabats_Info">

    <RelativeLayout
        android:id="@+id/content_info"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:elevation="50dp"
        android:ems="1">

        <RelativeLayout
            android:id="@+id/Weight_patient"
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_35sdp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/_10sdp"
            android:paddingStart="20dp"
            android:paddingEnd="20dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/cairo_bold"
                android:text="نوع المرض"
                android:textColor="@color/Patient_Color"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/FD_DiabetesType"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/cairo_bold"
                android:textColor="@color/gray"
                android:textSize="15sp" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/FD_DiabetesMedics_R"
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_35sdp"
            android:layout_below="@id/Weight_patient"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/_10sdp"
            android:paddingStart="20dp"
            android:paddingEnd="@dimen/_20sdp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/cairo_bold"
                android:text="نوع العلاج"
                android:textColor="@color/Patient_Color"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/FD_DiabetesMedics"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/cairo_bold"
                android:textColor="@color/gray"
                android:textSize="15sp" />

            <View style="@style/Divider.Horizontal" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/FD_PatientCauses_R"
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_35sdp"
            android:layout_below="@id/FD_DiabetesMedics_R"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/_10sdp"
            android:paddingStart="20dp"
            android:paddingEnd="@dimen/_20sdp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/cairo_bold"
                android:text="عامل الإصابة"
                android:textColor="@color/Patient_Color"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/FD_PatientCauses"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/cairo_bold"
                android:textColor="@color/gray"
                android:textSize="15sp" />

            <View style="@style/Divider.Horizontal" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/FD_bloodGlucoseMeter_R"
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_35sdp"
            android:layout_below="@id/FD_PatientCauses_R"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/_10sdp"
            android:paddingStart="20dp"
            android:paddingEnd="@dimen/_20sdp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/cairo_bold"
                android:text="هل تملك جهاز فحص السكر"
                android:textColor="@color/Patient_Color"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/FD_bloodGlucoseMeter"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/cairo_bold"
                android:textColor="@color/gray"
                android:textSize="15sp" />

            <View style="@style/Divider.Horizontal" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/FD_HaveIssue_R"
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_35sdp"
            android:layout_below="@id/FD_bloodGlucoseMeter_R"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/_10sdp"
            android:paddingStart="20dp"
            android:paddingEnd="@dimen/_20sdp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/cairo_bold"
                android:text=" تاريخ الاصابة"
                android:textColor="@color/Patient_Color"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/FD_HaveIssue"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/cairo_bold"
                android:textColor="@color/gray"
                android:textSize="15sp" />

            <View style="@style/Divider.Horizontal" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/FD_DateInjury_R"
            android:layout_width="@dimen/_250sdp"
            android:layout_height="@dimen/_35sdp"
            android:layout_below="@id/FD_HaveIssue_R"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="@dimen/_10sdp"
            android:fontFamily="@font/cairo_bold"
            android:paddingStart="20dp"
            android:paddingEnd="@dimen/_20sdp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/cairo_bold"
                android:text="أمراض اخرى "
                android:textColor="@color/Patient_Color"
                android:textSize="15sp" />

            <TextView
                android:id="@+id/FD_DateInjury"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/cairo_bold"
                android:textColor="@color/gray"
                android:textSize="15sp" />

            <View style="@style/Divider.Horizontal" />
        </RelativeLayout>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>